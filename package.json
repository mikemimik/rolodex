{
  "name": "rolodex",
  "version": "1.0.0",
  "description": "HackerYou Fullstack Masterclass Project",
  "main": "index.js",
  "scripts": {
    "deploy": "node api/server.js",
    "build": "yarn run build:client && yarn run move:client",
    "lint": "eslint .",
    "test": "react-scripts test",
    "start:server": "pm2 start process.config.js",
    "stop:server": "pm2 delete process.config.js",
    "logs:server": "pm2 logs /api/ --lines 10",
    "start:client": "react-scripts start",
    "build:client": "react-scripts build",
    "move:client": "cp -R build/ public",
    "git:remote:set": "git remote set-url origin git@github.com:mikemimik/rolodex.git"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikemimik/rolodex.git"
  },
  "keywords": [],
  "author": "Michael Perrotte",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HackerYou/con-ed-full-stack-student-notes/issues"
  },
  "homepage": "https://mikemimik-rolodex.herokuapp.com",
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "fast-redact": "^1.5.0",
    "init-package-json": "^1.10.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "mongoose": "^5.4.22",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-semistandard-react": "^4.2.0",
    "eslint-plugin-semistandard-react": "^4.1.0",
    "hackeryou-fullstack-react-scripts": "2.3.0",
    "pm2": "^3.5.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://localhost:4000",
  "semistandard": {
    "ignore": [
      "build/**"
    ]
  }
}
