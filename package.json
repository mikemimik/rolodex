{
  "name": "rolodex",
  "version": "1.0.0",
  "description": "HackerYou Fullstack Masterclass Project",
  "scripts": {
    "deploy": "npm run printenv && npm run build:client && node api/server.js",
    "build": "npm run build:client && npm run move:client",
    "printenv": "printenv",
    "lint": "eslint .",
    "test": "react-scripts test",
    "start:server:pm2": "pm2 start process.config.js",
    "stop:server:pm2": "pm2 delete process.config.js",
    "start:server": "nodemon api/server.js --ignore src --ignore public --ignore build",
    "logs:server": "pm2 logs /api/ --lines 10",
    "start:client": "react-scripts start",
    "build:client": "react-scripts build",
    "move:client": "cp -R build/ public",
    "git:remote:set": "git remote set-url origin git@github.com:mikemimik/rolodex.git"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikemimik/rolodex.git"
  },
  "keywords": [],
  "author": "Michael Perrotte",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HackerYou/con-ed-full-stack-student-notes/issues"
  },
  "homepage": "https://mikemimik-rolodex.herokuapp.com",
  "dependencies": {
    "@material-ui/core": "^4.7.0",
    "@material-ui/icons": "^4.5.1",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "fast-redact": "^2.0.0",
    "init-package-json": "^1.10.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.12",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^6.7.1",
    "eslint-config-semistandard-react": "^4.2.0",
    "eslint-plugin-semistandard-react": "^4.1.0",
    "nodemon": "^2.0.1",
    "pm2": "^4.2.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://localhost:4000",
  "semistandard": {
    "ignore": [
      "build/**"
    ]
  }
}
